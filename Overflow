<script src="../scripts/index.js"></script>
<h1>
<script type="text/javascript">
var d = new Date();

var time = d.getHours();

if (time<10)

{

document.write("Good morning! Welcome to Farm to Bar");


}

else if (time>=10 && time<16)

{

document.write("Good Afternoon! Welcome to Farm to Bar");



}

else

{

document.write("Good Evening! Welcome to Farm to Bar");


}

</script>
</h1>





